{
  "name": "@rikstv/shared-components",
  "version": "0.7.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "lib": "dist"
  },
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && run-p generate:type:*",
    "serve": "vite preview",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "generate:type:components": "tsc src/components/index.ts --declaration --emitDeclarationOnly --jsx react-jsx --esModuleInterop --outDir dist",
    "generate:type:tools": "tsc src/tools/tools.ts --declaration --emitDeclarationOnly --esModuleInterop --outDir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfbergee/rtv-ds.git"
  },
  "bugs": {
    "url": "https://github.com/fremtind/lfbergee/rtv-ds/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@cypress/react": "^5.10.0",
    "@cypress/vite-dev-server": "^2.1.0",
    "@testing-library/cypress": "^8.0.1",
    "case": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "react-hook-form": "^7.16.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-svg": "^2.0.0",
    "vite-plugin-react-svg": "^0.2.0"
  },
  "peerDependencies": {
    "@types/react": "^16.8.6 || ^17",
    "@types/react-dom": "^16.8.6 || ^17",
    "react": "^16.8.0 || ^17",
    "vite-plugin-style-import": "^1.2.1"
  }
}
